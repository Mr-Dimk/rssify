# Production environment variables for RSSify
# Пример безопасных настроек для продакшн

# FastAPI
APP_ENV=production
APP_DEBUG=0

# Database
DATABASE_URL=sqlite:///data/rssify.db

# Security
SECRET_KEY=change_this_to_a_long_random_string
ALLOWED_HOSTS=*

# Scheduler
SCHEDULER_TIMEZONE=UTC

# Logging
LOG_LEVEL=INFO
LOG_FILE=/logs/rssify.log

# Other
# Add more variables as needed
